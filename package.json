{
  "name": "tumau",
  "private": true,
  "keywords": [
    "node",
    "server",
    "ts"
  ],
  "repository": "git@github.com:etienne-dldc/tumau.git",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build:docs": "pnpm run export --scope docs",
    "build": "pnpm run build -r",
    "test": "pnpm run --stream --concurrency 1 test",
    "typecheck:all": "pnpm recursive exec --filter '@tumau/*' --filter '@tumau-example/*' -- pnpx tsc --noEmit",
    "norm": "echo 'TODO'",
    "norm:core": "echo 'TODO'",
    "benchmark": "ts-node --files --project ./scripts/tsconfig.json ./scripts/src/benchmark.ts",
    "pub": "yarn build && yarn test && changesets  && changesets publish",
    "lint": "eslint .",
    "generate-readme": "ts-node --files --project ./scripts/tsconfig.json ./scripts/src/generateReadme.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.11.2",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7"
  }
}
