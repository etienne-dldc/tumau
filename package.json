{
  "name": "tumau",
  "version": "1.0.0-alpha.93",
  "description": "A node HTTP framework written in Typescript",
  "keywords": [
    "node",
    "server",
    "ts"
  ],
  "repository": "git@github.com:etienne-dldc/tumau.git",
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/tumau.es.js",
      "require": "./dist/tumau.cjs.js"
    }
  },
  "main": "./dist/tumau.cjs.js",
  "module": "./dist/tumau.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && vite build && yarn build:types",
    "build:types": "tsc -p ./tsconfig.build.json",
    "example:run": "vite serve example",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "test": "yarn lint && jest --coverage",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "chemin": "^8.0.0",
    "miid": "^4.0.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/koa": "^2.13.4",
    "@types/node": "^16.7.6",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.0.6",
    "jest-ts-tsx-resolver": "^1.0.0",
    "koa": "^2.13.1",
    "node-fetch": "^2.6.1",
    "np": "^7.5.0",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5",
    "vite": "^2.4.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
