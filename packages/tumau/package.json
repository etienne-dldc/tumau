{
  "name": "tumau",
  "version": "1.0.0-alpha.90",
  "description": "A node HTTP framework written in Typescript",
  "keywords": [
    "node",
    "server",
    "ts"
  ],
  "repository": "git@github.com:etienne-dldc/tumau.git",
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/tumau.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@tumau/compress": "workspace:*",
    "@tumau/cookie": "workspace:*",
    "@tumau/core": "workspace:*",
    "@tumau/cors": "workspace:*",
    "@tumau/json": "workspace:*",
    "@tumau/rate-limit": "workspace:*",
    "@tumau/router": "workspace:*",
    "@tumau/static": "workspace:*",
    "@tumau/string-body": "workspace:*",
    "@tumau/url-parser": "workspace:*",
    "@tumau/ws": "workspace:*"
  },
  "devDependencies": {
    "tsdx": "^0.14.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdx build --target node",
    "start": "tsdx watch --noClean --target node"
  }
}
